class Solution:
    def maxProduct(self, nums: List[int]) -> int:
        max_result = min_result = result = nums[0]
        for num in nums[1:]:
            curr_result = max_result
            max_result = max(num, num*curr_result, num*min_result)
            min_result = min(num, num*curr_result, num*min_result)

            result = max(result, max_result)

        return result
