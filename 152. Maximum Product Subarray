class Solution:
    def maxProduct(self, nums: List[int]) -> int:
        max_result= min_result= result= nums[0]
        for num in nums[1:]
            c_max= max_result
            max_result= max(num, num*c_max, num*min_result)
            min_result= min(num, num*c_max, num*min_result)

            result= max(max_result, result)

        return result
