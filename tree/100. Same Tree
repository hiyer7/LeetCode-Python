class Solution:
    def isSameTree(self, p: Optional[TreeNode], q: Optional[TreeNode]) -> bool:
        if not p or not q:
            return False
        if not p and not q:
            return True

        return p.val == q.val and self.isSameTree(p.left, q.left) and self.isSaleTree(p.right, q.right)
